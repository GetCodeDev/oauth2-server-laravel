{
    "name": "lucadegasperi/oauth2-server-laravel",
    "type": "laravel-library",
    "description": "A Laravel 4 wrapper for the popular OAuth 2.0 Server package league/oauth2-server",
    "keywords": [
        "laravel",
        "oauth2",
        "oauth",
        "server",
        "api"
    ],
    "authors": [
        {
            "name": "Luca Degasperi",
            "email": "packages@lucadegasperi.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "league/oauth2-server": "2.1.x",
        "composer/installers": "^1.9"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "tests/TestCase.php"
        ],
        "psr-4": {
            "LucaDegasperi\\OAuth2Server\\": "src/LucaDegasperi/OAuth2Server"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LucaDegasperi\\OAuth2Server\\OAuth2ServerServiceProvider"
            ],
            "aliases":{
                "AuthorizationServer": "LucaDegasperi\\OAuth2Server\\Facades\\AuthorizationServerFacade",
                "ResourceServer": "LucaDegasperi\\OAuth2Server\\Facades\\ResourceServerFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "license": "MIT"
}